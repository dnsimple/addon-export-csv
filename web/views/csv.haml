%form{method: "POST"}
  %button.button{type: "submit"}
    Download CSV

.island
  %table
    %tr
      %th Domain Name
      %th State
      %th Expiration
      %th Whois privacy
      %th Auto renewal
    -@domains.each do |domain|
      %tr
        %td
          =domain.fetch("name")
        %td
          =domain.fetch("state")
        %td
          =domain.fetch("expires_on") || "-"
        %td
          =domain.fetch("private_whois")
        %td
          =domain.fetch("auto_renew")

